<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sep.utsloanapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.mainActivity.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.studentLoginActivity.StudentLoginActivity"
            android:label="@string/title_activity_student_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.logoActivity.LogoPageActivity"
            android:label="@string/title_activity_logo_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.staffLoginActivity.StaffLoginActivity"
            android:label="@string/title_activity_staff_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.settingsActivity.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.createFormActivity.CreateFormActivity"
            android:label="@string/title_activity_create_form"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.studentMyFormsActivity.StudentMyFormsActivity"
            android:label="@string/title_activity_student_my_forms"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.studentPersonalDetailActivity.StudentPersonalDetailActivity"
            android:label="@string/title_activity_student_personal_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.applicationDetailActivity.ApplicationDetailActivity"
            android:label="@string/title_activity_application_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.guideActivity.GuideActivity"
            android:label="@string/title_activity_guide"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>